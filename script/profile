#!/usr/bin/env bash

function _ubuntu_config_profile {
  local shome="${_ubuntu_config_home:-"$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"}"

  export PKG_HOME="$HOME"
  export SVDIR="$HOME/service"
  export BLOCK_PATH="$HOME/work"
  export NODE_VERSION="6.9.2"
  export BASEBOX_CACHE="/data"
}

_ubuntu_config_profile
